   ],
   "source": [
    "# undistort using cv.undistort()\n",
    "dst = cv2.undistort(img, matrix, distortion, None, newcameramtx)\n",
    "# crop the image\n",
    "x, y, w, h = roi\n",
    "dst = dst[y:y+h, x:x+w]\n",
    "cv2.imwrite(path +'1269(2).jpg', dst)"
   ]
  },
  {
